<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated dnd -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="test.dnd.presentation.DndModelWizardID"
            name="%_UI_DndModelWizard_label"
            class="test.dnd.presentation.DndModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/DndModelFile.gif">
         <description>%_UI_DndModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated dnd -->
      <editor
            id="test.dnd.presentation.DndEditorID"
            name="%_UI_DndEditor_label"
            icon="icons/full/obj16/DndModelFile.gif"
            extensions="dnd"
            class="test.dnd.presentation.DndEditor"
            contributorClass="test.dnd.presentation.DndActionBarContributor">
      </editor>
   </extension>

</plugin>
