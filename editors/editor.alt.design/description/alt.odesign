          <ownedTools xsi:type="tool:NodeCreationDescription" name="createTerminator" label="new Terminator" nodeMappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='Base']/@defaultLayer/@nodeMappings[name='TerminatorNode']">
