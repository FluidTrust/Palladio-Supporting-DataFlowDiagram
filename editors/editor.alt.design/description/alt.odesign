              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="createFunction" label="new Function" nodeMappings="//@ownedViewpoints[name='MyViewpoint']/@ownedRepresentations[name='Base']/@defaultLayer/@nodeMappings[name='FunctionNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
